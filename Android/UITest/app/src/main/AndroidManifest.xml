<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.se.uitest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name =".LoginActivity"
                   android:screenOrientation="landscape"/>
        <activity android:name =".JoinActivity"
                   android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_W"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_W_T"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_W_Ts"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_M"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_M_W"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_User_M_M"
                    android:screenOrientation="landscape"/>
        <activity android:name =".MenuActivity_Admin"/>
        <activity android:name =".MenuActivity_Admin_S"/>
        <activity android:name =".MenuActivity_Admin_L"/>


    </application>

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>