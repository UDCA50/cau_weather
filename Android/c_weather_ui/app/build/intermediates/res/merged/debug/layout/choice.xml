<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0000FF"
    >


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="208관"
        android:textSize="35dp"
        android:textStyle="bold"
        android:textColor="#000000"
        android:id="@+id/textView8"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="37dp" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="101호"
        android:id="@+id/button11"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/textView8"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="102호"
        android:id="@+id/button12"
        android:layout_alignBottom="@+id/button11"
        android:layout_toRightOf="@+id/button11"
        android:layout_toEndOf="@+id/button11" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="과사무실"
        android:id="@+id/button13"
        android:layout_below="@+id/button11"
        android:layout_toLeftOf="@+id/button12"
        android:layout_toStartOf="@+id/button12" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="514호"
        android:id="@+id/button14"
        android:layout_below="@+id/button11"
        android:layout_toRightOf="@+id/button11"
        android:layout_toEndOf="@+id/button11" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="515호"
        android:id="@+id/button15"
        android:layout_below="@+id/button13"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="6층PC실"
        android:id="@+id/button16"
        android:layout_alignTop="@+id/button15"
        android:layout_toRightOf="@+id/button15"
        android:layout_toEndOf="@+id/button15" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="4층PC실"
        android:id="@+id/button17"
        android:layout_below="@+id/button15"
        android:layout_alignLeft="@+id/button15"
        android:layout_alignStart="@+id/button15" />

    <Button
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="5층화장실"
        android:id="@+id/button18"
        android:layout_alignBottom="@+id/button17"
        android:layout_toRightOf="@+id/button15"
        android:layout_toEndOf="@+id/button15" />
</RelativeLayout>